var charMap = new Dictionary<char,char> {
	{'A', 'R'},
	{'B', 'P'},
	{'C', 'S'},
	{'X', 'R'},
	{'Y', 'P'},
	{'Z', 'S'},
};

var shapeScoreMap = new Dictionary<char, int> { 
	{'R', 1 },
	{'P', 2 },
	{'S', 3 }
};

// true when player (item 2 in the tuple) wins, otherwise false
// excludes draws because these can be tested without a map
var playerWinMap = new Dictionary<ValueTuple<char, char>, bool> {
	{('R', 'P'), true},
	{('R', 'S'), false},
	{('P', 'R'), false},
	{('P', 'S'), true},
	{('S', 'R'), true},
	{('S', 'P'), false},
};

input
	// split to lines
	.Split("\r\n")
	// split each line to char array with elements normalised to R/P/S for rock/paper/scissors
	.Select(
		line =>
			line.Split(' ')
			.Select(v => charMap[v[0]])
			.ToArray()
	)
	// deconstruct to tuple for convenience
	.Select(round => {
		return (round[0], round[1]);
	})
	// calculate the scores
	.Select(round => {
		var (opponent, player) = round;
		return shapeScoreMap[player] + ((player == opponent) ? 3 : (playerWinMap[round] ? 6 : 0));
	})
	// sum for answer
	.Sum()
	.Dump();