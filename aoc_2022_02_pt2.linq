var charMap = new Dictionary<char, char> {
	{'A', 'R'},
	{'B', 'P'},
	{'C', 'S'},
	{'X', 'L'},
	{'Y', 'D'},
	{'Z', 'W'},
};

var shapeScoreMap = new Dictionary<char, int> {
	{'R', 1 },
	{'P', 2 },
	{'S', 3 }
};

var gameScoreMap = new Dictionary<char, int> {
	{'W', 6 },
	{'D', 3 },
	{'L', 0 }
};

// maps opponent choice and win/lose to the shape to choose
// draws are not present because they can be resolved without a map
var playerMoveMap = new Dictionary<ValueTuple<char, char>, char> {
	{('R', 'W'), 'P'},
	{('P', 'W'), 'S'},
	{('S', 'W'), 'R'},
	{('R', 'L'), 'S'},
	{('P', 'L'), 'R'},
	{('S', 'L'), 'P'},
};

input
	// split to lines
	.Split("\r\n")
	// split each line to char array with elements normalised to R/P/S for rock/paper/scissors and W/D/L for win/draw/lose
	.Select(
		line =>
			line.Split(' ')
			.Select(v => charMap[v[0]])
			.ToArray()
	)
	// deconstruct to tuple for convenience
	.Select(round =>
	{
		return (round[0], round[1]);
	})
	// calculate the moves and scores
	.Select(round =>
	{
		var (opponent, desiredResult) = round;
		char player;
		if (desiredResult == 'D')
			player = opponent; // if it's a draw, pick the same shape as the opponent
		else
			player = playerMoveMap[round]; // otherwise, look up which move we should make
		
		return shapeScoreMap[player] + gameScoreMap[desiredResult];
	})
	// sum for answer
	.Sum()
	.Dump();
